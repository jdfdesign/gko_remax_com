<section class="page-header-section">
  <div class="container row">
    <div class="col-md-12">
      <div class="page-header">
        <%= render_page_title(parent.title) %>
    	  <%= render_html_text(parent.body) unless params[:page].present? %>
    	</div>
    </div>
  </div>    
</section>
<section class="projects-section">
	<!-- Begin Projects -->
	<div class="container">
    <div id="projects-container" class="row">
      <% if collection.any? %>
        <%= render collection %> 
      <% else %>
        <div class="col-md-12">
          <%= render "no_records_found" %>
        </div>
      <% end %>
    </div> 
    <% if collection.any? && collection.respond_to?(:total_entries) %>
    <div class="row pagination-container">
      <div class="col-xs-12">
        <%= will_paginate(collection) %>
      </div>
    </div>
    <% end %>
  </div>
</section>
<% content_for :sidebar do %>
	<%= render "sale_properties/filter", :section => parent %>
<% end %>